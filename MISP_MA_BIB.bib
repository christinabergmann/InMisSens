@article{Mani2007,
abstract = {Infants become selectively sensitive to phonological distinctions relevant to their native language at an early age. One might expect that infants bring some of this phonological knowledge to bear in encoding the words they subsequently acquire. In line with this expectation, studies have found that 14-month-olds are sensitive to mispronunciations of initial consonants of familiar words when asked to identify a referent. However, there is very little research investigating infants' sensitivity to vowels in lexical representations. Experiment 1 examines whether infants at 15, 18 and 24 months are sensitive to mispronunciations of vowels in familiar words. The results provide evidence for vowels constraining lexical recognition of familiar words. Experiment 2 compares 15, 18 and 24-month-olds' sensitivity to consonant and vowel mispronunciations of familiar words in order to assess the relative contribution of vowels and consonants in constraining lexical recognition. Our results suggest a symmetry in infants' sensitivity to vowel and consonant mispronunciations early in the second year of life. ?? 2007 Elsevier Inc. All rights reserved.},
author = {Mani, Nivedita and Plunkett, Kim},
doi = {10.1016/j.jml.2007.03.005},
file = {:C$\backslash$:/Users/user/Documents/Mendeley Desktop/Mani, Plunkett - 2007 - Phonological specificity of vowels and consonants in early lexical representations.pdf:pdf},
isbn = {0749-596X},
issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {Consonants,Lexical representation,Phonological specificity,Vowels},
number = {2},
pages = {252--272},
title = {{Phonological specificity of vowels and consonants in early lexical representations}},
volume = {57},
year = {2007}
}
