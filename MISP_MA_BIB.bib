@article{Bailey2002,
abstract = {Young children often fail to distinguish words differing by a single phoneme. It has been suggested that the phonological representations associated with early words are under-specified, with detail being added either as required to distinguish more words as a child's vocabulary increases, or as a function of the amount of exposure a child has had to a particular word. We report on an inter-modal preferential looking task in which 48, 18- and 24-month-olds heard either accurate pronunciations of picture names or slight (1- or 2-feature) mispronunciations. Each child was tested on four words that they learned at a very young age, and four words they had learned only recently relative to the test session. Across all children, 30 different words were tested. Analysis of children's looking behavior found differences between their responses to accurate and inaccurate pronunciations. These results show that toddlers' representations for words have enough detail to detect even slight mispronunciations. This ability was unaffected by their age, the size of their receptive vocabulary, the neighborhood density of individual test words, or the recency with which words had been learned. The results suggest that the specificity of lexical representations may be unrelated to any of these factors, at least within the range of ages and word familiarity examined in this study. ?? 2002 Elsevier Science Inc. All rights reserved.},
author = {Bailey, Todd M. and Plunkett, Kim},
doi = {10.1016/S0885-2014(02)00116-8},
file = {:C$\backslash$:/Users/user/Documents/Mendeley Desktop/Bailey, Plunkett - 2002 - Phonological specificity in early words.pdf:pdf},
isbn = {0885-2014},
issn = {08852014},
journal = {Cognitive Development},
keywords = {Auditory word recognition,Lexical representation,Phoneme development,Phonological specificity,Speech perception,Young children},
number = {2},
pages = {1265--1282},
title = {{Phonological specificity in early words}},
volume = {17},
year = {2002}
}
@article{Csibra2016,
abstract = {Looking times (LTs) are frequently measured in empirical research on infant cognition. We analyzed the statistical distribution of LTs across participants to develop recommendations for their treatment in infancy research. Our analyses focused on a common within-subject experimental design, in which longer looking to novel or unexpected stimuli is predicted. We analyzed data from 2 sources: an in-house set of LTs that included data from individual participants (47 experiments, 1,584 observations), and a representative set of published articles reporting group-level LT statistics (149 experiments from 33 articles). We established that LTs are log-normally distributed across participants, and therefore, should always be log-transformed before parametric statistical analyses. We estimated the typical size of significant effects in LT studies, which allowed us to make recommendations about setting sample sizes. We show how our estimate of the distribution of effect sizes of LT studies can be used to design experiments to be analyzed by Bayesian statistics, where the experimenter is required to determine in advance the predicted effect size rather than the sample size. We demonstrate the robustness of this method in both sets of LT experiments.},
author = {Csibra, Gergely and Hernik, Miko{\l}aj and Mascaro, Olivier and Tatone, Denis and Lengyel, M{\'{a}}t{\'{e}}},
doi = {10.1037/dev0000083},
file = {:C$\backslash$:/Users/user/Documents/Mendeley Desktop/Csibra et al. - 2016 - Statistical treatment of looking-time data.pdf:pdf},
journal = {Developmental Psychology},
keywords = {10,1037,bayesian statistics,dev0000083,doi,dx,http,infancy,is one of the,log-normal distribution,looking times,lts,lts are mea-,measuring looking times,most frequently,org,research with infants,supp,supplemental materials,used behavioral techniques in},
number = {4},
pages = {521--36},
title = {{Statistical treatment of looking-time data}},
volume = {52},
year = {2016}
}
@article{Jennions2002,
abstract = {Both signi{\textregistered}cant positive and negative relationships between the magnitude of research {\textregistered}ndings (their `effect size') and their year of publication have been reported in a few areas of biology. These trends have been attributed to Kuhnian paradigm shifts, scienti{\textregistered}c fads and bias in the choice of study systems. Here we test whether or not these isolated cases re¯ect a more general trend. We examined the relationship using effect sizes extracted from 44 peer-reviewed meta-analyses covering a wide range oftopics in ecological and evolutionary biology. On average, there was a small but signi{\textregistered}cant decline in effect size with year of publication. For the original empirical studies there was also a signi{\textregistered}cant decrease in effect size as sample size increased. However, the effect of year of publication remained even after we controlled for sampling effort. Although these results have several possible explanations, it is suggested that a publication bias against non-signi{\textregistered}cant or weaker {\textregistered}ndings offers the most parsimonious explanation. As in the medical sciences, non-signi{\textregistered}cant results may take longer to publish and studies with both small sample sizes and non-signi{\textregistered}cant results may be less likely to be published.},
author = {Jennions, Michael D and M{\`{u}}, Anders P and Pierre, {\^{A}} and Curie, Marie and Cedex, F- Paris},
doi = {10.1098/rspb.2001.1832},
file = {:C$\backslash$:/Users/user/Documents/Mendeley Desktop/Jennions et al. - 2002 - Relationships fade with time a meta-analysis of temporal trends in publication in ecology and evolution.pdf:pdf},
journal = {Proceedings of the Royal Society of London B: Biological Sciences},
keywords = {ecology,evolution,meta-analysis,paradigms,publication bias},
pages = {43--48},
title = {{Relationships fade with time : a meta-analysis of temporal trends in publication in ecology and evolution}},
volume = {269},
year = {2002}
}
