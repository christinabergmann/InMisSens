file = {:C$\backslash$:/Users/user/Documents/Mendeley Desktop/Altvater-Mackensen, van der Feest, Fikkert - 2014 - Asymmetries in early word recognition The case of stops and fricatives.pdf:pdf},
issn = {1547-5441},
